    "cookie-parser": "~1.3.5",
    "cucumber": "^0.8.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "jade": "~1.11.0",
    "materialize-css": "^0.97.2",
    "mocha": "^2.3.3",
    "morgan": "~1.6.1",
    "passport": "^0.3.0",
    "passport-hash": "^0.5.0",
    "passport-local": "^1.0.0",
    "redux": "^3.0.4",
    "riot": "^2.3.1",
    "serve-favicon": "~2.3.0",
    "zombie": "^4.2.1"
  }
